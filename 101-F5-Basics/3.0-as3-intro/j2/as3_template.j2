"web_app": {
    "class": "Application",
    "template": "http",
    "serviceMain": {
        "class": "Service_HTTP",
        "virtualAddresses": [
            "{{ private_ip }}"
        ],
        "profileMultiplex": {
            "bigip": "/Common/oneconnect"
        },
        "pool": "app_pool",
        "persistenceMethods": []
    },
    "app_pool": {
        "class": "Pool",
        "monitors": [
            "http"
        ],
        "members": [
            {
                "servicePort": 443,
                "serverAddresses": [
                    {% set comma = joiner(",") %}
                    {% for mem in pool_members %}
                        {{comma()}} "{{  hostvars[mem]['private_ip']  }}"
                    {% endfor %}

                ]
            }
        ]
    }
}
